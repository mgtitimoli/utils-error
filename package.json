{
  "name": "@mgtitimoli/utils-error",
  "description": "Error Utilities",
  "private": true,
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Mauro Gabriel Titimoli <mgtitimoli@gmail.com> (https://github.com/mgtitimoli)",
  "license": "MIT",
  "scripts": {
    "build": "run-s build:clean build:code",
    "build:clean": "rm -rf dist",
    "build:code": "ttsc --inlineSources --sourceMap --sourceRoot '.'",
    "commit": "commit",
    "debug": "node --require ts-node/register --require tsconfig-paths/register --inspect src/index.ts",
    "lint": "run-s lint:types lint:code",
    "lint:fix": "run-s lint:types lint:fix:code",
    "lint:fix:code": "yarn run lint:code --fix",
    "lint:code": "eslint src",
    "lint:types": "ttsc --noEmit",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@commitlint/prompt-cli": "^9.1.1",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "concat-md": "^0.3.5",
    "doctoc": "^1.4.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "np": "^6.4.0",
    "npm-run-all": "^4.1.5",
    "pkg-to-readme": "^1.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typedoc": "^0.17.8",
    "typedoc-plugin-example-tag": "^1.0.2",
    "typedoc-plugin-external-module-map": "^1.2.1",
    "typedoc-plugin-internal-external": "^2.2.0",
    "typedoc-plugin-markdown": "^2.3.1",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  }
}
